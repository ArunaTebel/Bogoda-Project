     gl_cash_advance.`date_time` AS gl_cash_advance_date_time,
     gl_cash_advance.`user` AS gl_cash_advance_user
FROM
     `gl_cash_advance` gl_cash_advance
WHERE
     issued_date >=$P{from_date} AND  issued_date <=$P{to_date} AND sup_id = $P{quary}]]>
