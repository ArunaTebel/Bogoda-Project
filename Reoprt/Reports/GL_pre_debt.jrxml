	<queryString>
		<![CDATA[SELECT
     supplier_pre_debt_coins.`sup_id` AS supplier_pre_debt_coins_sup_id,
     supplier_pre_debt_coins.`pre_debts` AS supplier_pre_debt_coins_pre_debts,
     supplier_pre_debt_coins.`coins` AS supplier_pre_debt_coins_coins,
     supplier_pre_debt_coins.`month` AS supplier_pre_debt_coins_month,
     suppliers.`sup_name` AS suppliers_sup_name,
     category.`category_name` AS category_category_name
FROM
     `category` category INNER JOIN `suppliers` suppliers ON category.`category_id` = suppliers.`cat_id`
     INNER JOIN `supplier_pre_debt_coins` supplier_pre_debt_coins ON suppliers.`sup_id` = supplier_pre_debt_coins.`sup_id`
WHERE
     month = $P{month} AND pre_debts != 0
order by category.`category_name`,supplier_pre_debt_coins.`pre_debts` DESC]]>
	</queryString>
